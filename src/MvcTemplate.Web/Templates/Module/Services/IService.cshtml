@model ModuleModel
using MvcTemplate.Objects;
using System;
using System.Linq;
using System.Threading.Tasks;

namespace MvcTemplate.Services
{
    public interface @Model.IService : IService
    {
        Task<TView> Get<TView>(Int32 id) where TView : BaseView;
        IQueryable<@Model.View> GetViews();

        Task Create(@Model.View view);
        Task Edit(@Model.View view);
        Task Delete(Int32 id);
    }
}
